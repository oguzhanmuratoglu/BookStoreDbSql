CREATE DATABASE [BookwormDb]
 CONTAINMENT = NONE
 ON  PRIMARY 
( NAME = N'BookwormDb', FILENAME = N'C:\Users\Oguzhan\BookwormDb.mdf' , SIZE = 8192KB , MAXSIZE = UNLIMITED, FILEGROWTH = 65536KB )
